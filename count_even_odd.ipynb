   "source": [
    "numbers = [10, 23, 45, 66, 78, 91, 100]\n",
    "\n",
    "even = 0\n",
    "odd = 0\n",
    "\n",
    "for n in numbers:\n",
    "    if n % 2 == 0:\n",
    "        even += 1\n",
    "    else:\n",
    "        odd += 1\n",
    "\n",
    "print(\"Even numbers:\", even)\n",
    "print(\"Odd numbers:\", odd)\n"
   ]
  }
