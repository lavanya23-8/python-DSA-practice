   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n - 1)\n",
    "\n",
    "num = 7\n",
    "print(\"Factorial of\", num, \"is:\", factorial(num))\n",
    "        "
   ]
