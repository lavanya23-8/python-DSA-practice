   "source": [
    "\n",
    "arr = [10, 20, 30, 40, 50]\n",
    "target = int(input(\"Enter number to search: \"))\n",
    "\n",
    "found = False\n",
    "for i in range(len(arr)):\n",
    "    if arr[i] == target:\n",
    "        print(f\"{target} found at index {i}\")\n",
    "        found = True\n",
    "        break\n",
    "\n",
    "if not found:\n",
    "    print(f\"{target} not found in the array.\")\n"
   ]
  }
