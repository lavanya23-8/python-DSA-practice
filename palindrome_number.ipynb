    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "121 is a Palindrome \n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "temp = num\n",
    "rev = 0\n",
    "\n",
    "while temp > 0:\n",
    "    rev = rev * 10 + temp % 10\n",
    "    temp //= 10\n",
    "\n",
    "if rev == num:\n",
    "    print(f\"{num} is a Palindrome \")\n",
    "else:\n",
    "    print(f\"{num} is not a Palindrome \")\n"
   ]
