    "numbers = [12, 45, 2, 67, 33, 89, 1]\n",
    "\n",
    "largest = numbers[0]\n",
    "smallest = numbers[0]\n",
    "\n",
    "for n in numbers:\n",
    "    if n > largest:\n",
    "        largest = n\n",
    "    if n < smallest:\n",
    "        smallest = n\n",
    "\n",
    "print(\"Largest number:\", largest)\n",
    "print(\"Smallest number:\", smallest)\n"
   ]
